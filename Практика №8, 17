import java.util.Scanner;

public class Classelement78 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Максимальное значение: " + processnode43(scanner));
        scanner.close();
    }

    public static int processnode43(Scanner scanner) {
        int sum19 = scanner.nextInt();
        if (sum19 == 0) {
            return 0;
        }
        return Math.max(sum19, processnode43(scanner));
    }
}
