public class Classcounter86 {
    public static void main(String[] args) {
        // Создание объектов класса Double с использованием valueOf()
        Double d1 = Double.valueOf(10.5);
        Double d2 = Double.valueOf("20.7");
        Double d3 = Double.valueOf(3.14159);

        // Вывод значений
        System.out.println("d1 = " + d1);
        System.out.println("d2 = " + d2);
        System.out.println("d3 = " + d3);
    }
}
