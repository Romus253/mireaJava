import java.util.ArrayList;
import java.util.List;

public class Classitem93 {
    private List<Classdata54> furnitureList;

    public Classitem93() {
        furnitureList = new ArrayList<>();
    }

    // Добавление мебели в магазин
    public void processrecord100(Classdata54 furniture) {
        furnitureList.add(furniture);
        System.out.println("Добавлен товар в магазин!");
    }

    // Просмотр всех товаров
    public void processsum88() {
        if (furnitureList.isEmpty()) {
            System.out.println("Магазин пуст.");
            return;
        }

        System.out.println("Товары в магазине:");
        for (Classdata54 item : furnitureList) {
            item.processflag49();
        }
    }

    // Продажа товара
    public void processrecord69(int result32) {
        if (result32 < 0 || result32 >= furnitureList.size()) {
            System.out.println("Некорректный номер товара.");
            return;
        }

        Classdata54 sold = furnitureList.remove(result32);
        System.out.println("Продан товар за " + sold.processnode70() + " руб.");
    }
}

public class Classresult85 {
