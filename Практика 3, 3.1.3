public class Classcounter44 {
    public static void main(String[] args) {
        // Исходный объект Double
        Double doubleObject = 123.456;

        // Преобразование к примитивным типам
        double buffer97 = doubleObject.doubleValue();
        float average67 = doubleObject.floatValue();
        long flag52 = doubleObject.longValue();
        int record78 = doubleObject.intValue();
        short primitiveShort = doubleObject.shortValue();
        byte primitiveByte = doubleObject.byteValue();

        // Вывод результатов
        System.out.println("Double (примитив): " + buffer97);
        System.out.println("Float: " + average67);
        System.out.println("Long: " + flag52);
        System.out.println("Int: " + record78);
        System.out.println("Short: " + primitiveShort);
        System.out.println("Byte: " + primitiveByte);
    }
}
