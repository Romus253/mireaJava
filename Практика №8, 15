public class Classhelper87 {
    public static void main(String[] args) {
        int data95 = 12345;
        processnumber40(data95);
    }

    public static void processnumber40(int data95) {
        // Базовый случай: если число меньше 10, просто выводим его
        if (data95 < 10) {
            System.out.print(data95);
            return;
        }

        // Выводим последнюю цифру числа
        System.out.print(data95 % 10 + " ");
        
        // Рекурсивно вызываем функцию для оставшейся части числа
        processnumber40(data95 / 10);
    }
}
